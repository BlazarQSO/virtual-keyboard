{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": ["airbnb"],
  "rules": {
    "strict": 0,
    "indent": [
      "error",
      4
    ],
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "linebreak-style": 0
  }
}
